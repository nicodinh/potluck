{
  "name": "potluck",
  "main": "index.js",
  "version": "0.0.3",
  "description": "Video chat one on one",
  "license": "MIT",
  "repository": "eaze/potluck",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "standard",
    "build": "browserify test.js -o out.js -p [ parcelify -o out.css ]",
    "dev": "watchify test.js -v -o out.js -p [ parcelify -wo out.css ]"
  },
  "browserify-shim": {
    "opentok": "global:OT"
  },
  "style": "style.css",
  "browserify": {
    "transform": [
      "browserify-shim",
      "autoprefixer-stream"
    ]
  },
  "dependencies": {
    "after": "^0.8.1",
    "mercury": "^14.0.0",
    "next-tick": "^0.2.2",
    "weakmap-event": "0.0.2",
    "winresize-event": "^0.2.1"
  },
  "devDependencies": {
    "autoprefixer-stream": "0.0.2",
    "browserify": "10.2.4",
    "browserify-css": "0.6.1",
    "browserify-shim": "3.8.9",
    "parcelify": "^1.1.3",
    "standard": "4.0.0",
    "tape": "4.0.0"
  },
  "files": [
    "index.js"
  ]
}
