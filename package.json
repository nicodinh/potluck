{
  "name": "potluck",
  "main": "index.js",
  "version": "0.0.0",
  "description": "Video chat one on one",
  "license": "MIT",
  "repository": "eaze/potluck",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "standard && tape test.js",
    "build": "browserify index.js -o out.js -p [ parcelify -wo out.css ]"
  },
  "transforms": [
    "autoprefixer-stream"
  ],
  "browserify-shim": {
    "opentok": "global:OT",
    "cordova": "global:cordova"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "style": "*.css",
  "dependencies": {
    "base-element": "3.0.1",
    "potluck-tokbox-adapter": "eaze/potluck-tokbox-adapter",
    "run-parallel": "1.1.2"
  },
  "devDependencies": {
    "autoprefixer-stream": "0.0.1",
    "browserify": "^10.2.4",
    "browserify-css": "^0.6.1",
    "browserify-shim": "^3.8.9",
    "parcelify": "^1.1.3",
    "standard": "^4.0.0",
    "tape": "^4.0.0"
  },
  "files": [
    "index.js"
  ]
}
